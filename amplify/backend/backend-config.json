		},
		"PortalsAppAuthDefineAuthChallenge": {
			"build": true,
			"providerPlugin": "awscloudformation",
			"service": "Lambda"
		},
		"PortalsAppAuthPostAuthentication": {
			"build": true,
			"providerPlugin": "awscloudformation",
